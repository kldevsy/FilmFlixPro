{
  "name": "rest-express",
  "version": "1.0.0",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "dev": "tsx server/index.ts",
    "build:server": "tsc --outDir dist/server --module ESNext --target ES2022 --esModuleInterop server/**/*.ts",
    "build:client": "vite build",
    "build": "npm run build:server && npm run build:client",
    "start": "node dist/server/index.js",
    "check": "tsc",
    "db:push": "drizzle-kit push"
  },
  "dependencies": {
    "@neondatabase/serverless": "^0.10.4",
    "express": "^4.21.2",
    "express-session": "^1.18.1",
    "drizzle-orm": "^0.39.1",
    "drizzle-zod": "^0.7.0",
    "@vercel/node": "^5.3.23",
    "ws": "^8.18.0",
    "zod": "^3.24.2",
    "zod-validation-error": "^3.4.0"
  },
  "devDependencies": {
    "typescript": "5.6.3",
    "tsx": "^4.19.1",
    "vite": "^5.4.19",
    "@vitejs/plugin-react": "^4.3.2",
    "esbuild": "^0.25.0"
  }
}